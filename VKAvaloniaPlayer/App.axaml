<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:VKAvaloniaPlayer"
             x:Class="VKAvaloniaPlayer.App">
    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>
    <Application.Resources>
		
		<SolidColorBrush x:Key="AccentColor" Color="#2888F5"/>
		<SolidColorBrush x:Key="BgColor" Color="White"/>
		<SolidColorBrush x:Key="TextColor" Color="Black"/>
		
		<SolidColorBrush x:Key="MenuTextColor" Color="White"/>
		<SolidColorBrush x:Key="MenuSelectedTextColor" Color="Black"/>
		<SolidColorBrush x:Key="MenuSelectedColor" Color="White"/>
		
		<SolidColorBrush x:Key="SliderTrackColor" Color="Gray"/>
		<SolidColorBrush x:Key="SubTitleColor" Color="Gray"/>
		
		<SolidColorBrush x:Key="ControlsColor" Color="#CDCDCD"/>
		<SolidColorBrush x:Key="MouseOverColor" Color="Gray"/>
		
		<SolidColorBrush x:Key="BlockedColor" Color="Gray"/>
		
		
    </Application.Resources>
	<Application.Styles>
		<FluentTheme Mode="Light" />
	</Application.Styles>

   
</Application>