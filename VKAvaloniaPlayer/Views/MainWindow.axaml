<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:VKAvaloniaPlayer.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		 xmlns:views="using:VKAvaloniaPlayer.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="VKAvaloniaPlayer.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="VKAvaloniaPlayer">
	<Window.Resources>
		<vm:VKLoginControlViewModel x:Key="LoginViewDataContext" ></vm:VKLoginControlViewModel>
	</Window.Resources>
    <Design.DataContext>
        <vm:MainWindowViewModel/>
    </Design.DataContext>

	<views:VKLoginControl DataContext="{StaticResource LoginViewDataContext}"></views:VKLoginControl>

</Window>
